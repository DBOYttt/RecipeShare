<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:conv="clr-namespace:RecipeShare.Converters"
             x:Class="RecipeShare.App">
  <Application.Resources>
    <ResourceDictionary>
      <conv:FavoriteTextConverter x:Key="FavoriteTextConverter" />
      <conv:FavoriteIconConverter x:Key="FavoriteIconConverter" />
      <conv:FavoriteBackgroundConverter x:Key="FavoriteBackgroundConverter" />
    </ResourceDictionary>
  </Application.Resources>
</Application>
